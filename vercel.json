{
    "builds": [
        {
            "src": "listiamo/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "Python3.9"
            }
        },
        {
            "src": "build.sh", 
            "use": "@vercel/static-build", 
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "listiamo/wsgi.py"
        }
    ],
    "git": {
        "deploymentEnabled": {
            "main": true,
            "dev": false
        }
    }
}